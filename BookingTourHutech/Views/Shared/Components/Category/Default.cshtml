


<div class="col-lg-12">
    <div class="mb-3">
        <h4>Theo Thành Phố</h4>
        <ul class="list-unstyled fruite-categorie">
            @foreach (var item in Model)
            {
                <li>
                    <div class="d-flex justify-content-between fruite-name">
                        <a asp-controller="Tour" asp-action="Index" as asp-route-loai="@item.CategoryTourId">

                            <i class="fas  me-2"></i>@item.CategoryName
                        </a>
                        <span>(@item.TotalCategory)</span>
                    </div>
                </li>
            }
        </ul>
    </div>
</div>
