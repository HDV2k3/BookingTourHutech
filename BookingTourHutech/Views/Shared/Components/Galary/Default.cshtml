<section class="gallary" id="gallary">
    <div class="container">

        <div class="main-txt">
            <h1><span>Bộ Sưu</span> Tập</h1>
        </div>

        <div class="row" style="margin-top: 30px;">
           @foreach(var item in Model)
            {
                <div class="col-md-4 py-3 py-md-0">
                    <div class="card">
                        <img src="~/images/@item.Images" alt="" height="230px">
                    </div>
                </div>
            }

        </div>       
    </div>
</section>